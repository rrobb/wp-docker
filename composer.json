{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "5.3.2",
        "dist": {
          "type": "zip",
          "url": "https://wordpress.org/latest.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "^1.0"
        }
      }
    }
  ],
  "extra": {
    "installer-paths": {
      "app/content/plugins/{$name}/": [
        "type:wordpress-plugin",
        "endouble/content-alert"
      ]
    },
    "webroot-dir": "app/wp",
    "webroot-package": "wordpress/wordpress"
  },
  "require": {
    "psr/container": "1.0.*",
    "wordpress/wordpress": "^5.3.2",
    "timber/timber": "^1.10",
    "twig/twig": "^2.11"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
